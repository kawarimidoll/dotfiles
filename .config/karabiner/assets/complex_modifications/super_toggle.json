{
  "title": "Toggle between Super+5 and Super+6",
  "description": "This is for toggle super keymaps in Raycast",
  "rules": [
    {
      "description": "Super+up toggles between Super+5 and Super+6",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["command", "shift", "option", "control"]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": ["command", "shift", "option", "control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "toggle_super",
              "value": 0
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "toggle_super",
                "value": 1
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["command", "shift", "option", "control"]
            }
          },
          "to": [
            {
              "key_code": "6",
              "modifiers": ["command", "shift", "option", "control"]
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "toggle_super",
              "value": 1
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "toggle_super",
                "value": 0
              }
            }
          ]
        }
      ]
    }
  ]
}
